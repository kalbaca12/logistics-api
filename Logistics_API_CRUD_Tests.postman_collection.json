          "name": "Register (201) admin",
          "event": [
            {
              "listen": "prerequest",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "if(!pm.environment.get('EMAIL')) pm.environment.set('EMAIL', `u${Date.now()}@example.com`);",
                  "if(!pm.environment.get('PASSWORD')) pm.environment.set('PASSWORD', 'secret1234');"
